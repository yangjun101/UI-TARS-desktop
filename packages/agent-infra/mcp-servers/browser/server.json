        },
        {
          "type": "named",
          "name": "proxy-server",
          "description": "Proxy server address",
          "format": "string",
          "is_required": false
        },
        {
          "type": "named",
          "name": "vision",
          "description": "Run server that uses screenshots (Aria snapshots are used by default)",
          "format": "boolean",
          "is_required": false
        }
      ],
      "environment_variables": [
        {
          "description": "DISPLAY environment variable for browser rendering",
          "is_required": false,
          "format": "string",
          "is_secret": false,
          "name": "DISPLAY"
        }
      ]
    }
  ]
}
